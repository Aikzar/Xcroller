{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "sql:default",
    "fs:default",
    "fs:read-all",
    "core:window:allow-set-fullscreen",
    "core:window:allow-is-fullscreen",
    "core:path:default"
  ]
}